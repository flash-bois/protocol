{
  "name": "protocol-v1",
  "version": "0.0.1",
  "main": "index.ts",
  "author": "fashbois",
  "scripts": {
    "start-sdk": "ts-node sdk/src/index.ts",
    "build:wasm:dev": "cd ./programs/wasm && wasm-pack build --target nodejs",
    "build:wasm:prod": "cd ./programs/wasm && wasm-pack build --target nodejs --out-dir lib --release",
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "test:sdk": "ts-mocha -t 100000 tests/sdk/*",
    "test:anchor": "anchor test tests/anchor/*.ts",
    "test:anchor:create_state": "anchor test tests/anchor/create_state.ts",
    "wasm": "cd ./programs/wasm && wasm-pack build --target nodejs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flash-bois/protocol-v1.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flash-bois/protocol-v1/issues"
  },
  "homepage": "https://github.com/flash-bois/protocol-v1#readme",
  "dependencies": {
    "@project-serum/anchor": "^0.26.0",
    "@solana/web3.js": "^1.73.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.3.5"
  }
}